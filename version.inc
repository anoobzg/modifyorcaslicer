# Included by CMakeLists, edited by the build script
# (the version numbers are generated by the build script from the git current label)

set(SLIC3R_APP_NAME "LightMaker")
set(SLIC3R_APP_KEY "LightMaker")

if(NOT DEFINED BBL_RELEASE_TO_PUBLIC)
set(BBL_RELEASE_TO_PUBLIC "1")
endif()
set(LightMaker_VERSION "2.3.0")
string(REGEX MATCH "^([0-9]+)\\.([0-9]+)\\.([0-9]+)"
       SoftFever_VERSION_MATCH ${LightMaker_VERSION})
set(LightMaker_VERSION_MAJOR ${CMAKE_MATCH_1})
set(LightMaker_VERSION_MINOR ${CMAKE_MATCH_2})
set(LightMaker_VERSION_PATCH ${CMAKE_MATCH_3})

set(SLIC3R_VERSION "01.10.01.50")
